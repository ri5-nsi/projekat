		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:AbsolutePanel height="201px" width="343px">
			<g:at left="30" top="45">
				<g:Label text="Name"/>
			</g:at>
			<g:at left="10" top="10">
				<g:Label text="Create Kanban Workflow"/>
			</g:at>
			<g:at left="70" top="45">
				<g:TextBox width="159px" height="6px" ui:field="workflowName"/>
			</g:at>
			<g:at left="265" top="157">
				<g:Button ui:field="createWorkflowBtn">Create</g:Button>
			</g:at>
		</g:AbsolutePanel>
	</g:HTMLPanel>
</ui:UiBinder> 